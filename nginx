  302  curl http://dl.fedoraproject.org/pub/epel/7/x86_64/Packages/e/epel-release-7-11.noarch.rpm -O
  303  ls -ltr
  304  rpm -ivh epel-release-7-11.noarch.rpm
  305  ls -ltr
  306  rm -r *.rpm
  307  vi epel.repo
  308  yum update
  309  who
  310  w
